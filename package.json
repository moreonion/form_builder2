    "element-ui": "1.3.7",
    "es6-promise": "4.1.0",
    "lodash.capitalize": "^4.2.1",
    "lodash.debounce": "^4.0.8",
    "mo-vue-dnd": "0.4.4",
    "vue": "2.3.4",
    "vuex": "2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.2",
