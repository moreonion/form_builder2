    "body-parser": "^1.18.3",
    "chromedriver": "2.36.0",
    "concurrently": "^3.6.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.18.0",
    "eslint-config-standard": "^11.0.0",
